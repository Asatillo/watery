{
    "manifest_version": 3,
    "name": "Watery",
    "version": "1.0",
    "description": "Reminds you to drink water regularly.",
    "permissions": [
        "storage",
        "notifications",
        "alarms"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}